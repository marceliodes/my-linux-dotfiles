#### XFT Setting ####
Xft.dpi:		96
Xft.antialias:		true
Xft.rgba:		rgb
Xft.hinting:		true
Xft.hintstyle:		hintslight

#### URXVT Setting ####
URxvt.depth:		32
URxvt.geometry:		90x30
URxvt.loginShell:	true
URxvt.internalBorder:	3
URxvt.lineSpace:	0

#### Transparency (true or false) ####
URxvt*transparent: 	false

URxvt*shading:		15

Urxvt*saveLines:	12000


#### Font ####
URxvt*font:		xft:JetBrainsMono-Regular:pixelsize=12
URxvt*boldfont:		xft:JetBrainsMono-Regular:Bold:pixelsize=12
URxvt*letterspace:	-1

#### Scrollbar (true or false) this is true on default ####
URxvt*scrollBar:	false


